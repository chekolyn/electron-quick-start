{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "client:generate": "electric-sql generate",
    "client:watch": "electric-sql generate --watch",
    "db:migrate": "electric-sql with-config \"pg-migrations apply --database {{ELECTRIC_PROXY}} --directory ./db/migrations\"",
    "db:test": "electric-sql with-config \"echo {{ELECTRIC_PROXY}}\"",
    "db:psql": "electric-sql psql"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^30.0.8",
    "@databases/pg-migrations": "^5.0.3"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.1.5",
    "electric-sql": "^0.11.3",
    "electron-rebuild": "^3.2.9",
    "pg": "^8.11.5",
    "zod": "3.21.1"
  }
}
